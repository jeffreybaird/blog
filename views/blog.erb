<h1>Hi. I'm <%= link_to("/","Jeff") %></h1>


<% posts.each do |post| %>
  <% next if post.internal_link == "the_world_turned_upside_down" %>
  <h3><%= link_to("blog/posts/#{post.internal_link}", post.title) %></h2>
  <h4>Published on: <%= post.date %></h3>
  <h4>By: <%= post.author %></h3>
  <p><%= post.description %></p>
<% end %>

